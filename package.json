{
    "name": "bash-notebook",
    "displayName": "Bash notebook",
    "description": "Notebook for bash",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onNotebook:bash-notebook"
    ],
    "files": [
        "./out/*"
    ],
    "main": "./out/extension/extension.js",
    "contributes": {
        "notebooks": [
            {
                "id": "bash-notebook",
                "type": "bash-notebook",
                "displayName": "Bash notebook",
                "selector": [
                    {
                        "filenamePattern": "*.bashbook"
                    }
                ]
            }
        ],
        "notebookRenderer": [
            {
                "id": "bash-notebook-renderer",
                "entrypoint": "./out/renderer/renderer.js",
                "displayName": "Bash notebook renderer",
                "mimeTypes": [
                    "x-application/bash-notebook"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm run compile:extension && npm run compile:renderer",
        "compile:extension": "tsc -p ./src/extension",
        "compile:renderer": "tsc -p ./src/renderer",
        "watch": "concurrently -r \"npm:watch:*\"",
        "watch:extension": "tsc --watch -p ./src/extension",
        "watch:renderer": "tsc --watch -p ./src/renderer"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/vscode-notebook-renderer": "^1.57.8",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "typescript": "^4.4.4",
        "concurrently": "^7.0.0"
    }
}
