{
    "name": "bash-notebook",
    "displayName": "Bash notebook",
    "description": "Notebook for bash",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onNotebook:bash-notebook"
    ],
    "main": "./out/extension.js",
    "files": [
        "./out/*"
    ],
    "contributes": {
        "notebooks": [
            {
                "id": "bash-notebook",
                "type": "bash-notebook",
                "displayName": "Bash notebook",
                "selector": [
                    {
                        "filenamePattern": "*.bashbook"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "typescript": "^4.4.4"
    }
}